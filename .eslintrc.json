{
	"env": {
		"es2021": true
	},
	"extends": ["standard-with-typescript", "plugin:react/recommended", "prettier"],
	"parserOptions": {
		"ecmaVersion": "latest",
		"sourceType": "module"
	},
	"plugins": ["react", "prettier"],
	"rules": {
		"prettier/prettier": [
			"error",
			{
				"endOfLine": "auto"
			}
		],
		"no-console": "warn",
		"@typescript-eslint/consistent-type-assertions": "off",
		"@typescript-eslint/strict-boolean-expressions": "off"
	}
}
